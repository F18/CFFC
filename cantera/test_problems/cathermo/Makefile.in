	cd ims; @MAKE@ -s test
	cd testIAPWS; @MAKE@ -s test
	cd testIAPWSPres; @MAKE@ -s test
	cd testIAPWSTripP; @MAKE@ -s test
	cd testWaterPDSS; @MAKE@ -s test
