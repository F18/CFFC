###############################################################
# list your object files here
# This is probably the only thing you need to change in this file.

OBJS    = user.o

###############################################################



SUFFIXES=
SUFFIXES= .cpp .d .o

OBJDIR = .

CXX_FLAGS = @CXXFLAGS@ $(CXX_OPT)


CXX_INCLUDES = -I../src

LIB = @buildlib@/libuser.a

DEPENDS = $(OBJS:.o=.d)

%.d:
	g++ -MM $(CXX_INCLUDES) $*.cpp > $*.d

.cpp.o:
	@CXX@ -c $< $(CXX_FLAGS) $(CXX_INCLUDES)

.f.o:
	@F77@ -c $< $(F77_FLAGS) 

all lib: $(LIB)

$(LIB): $(OBJS)
	@ARCHIVE@ $(LIB) $(OBJS) > /dev/null

clean:
	$(RM) *.o *~ $(LIB)

depends: $(DEPENDS)
	cat *.d > .depends
	$(RM) $(DEPENDS) 

install:


TAGS: 
	etags *.h *.cpp

ifeq ($(wildcard .depends), .depends)
include .depends
endif

