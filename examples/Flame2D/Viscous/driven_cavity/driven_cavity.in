#
Time_Integration_Type =
Multistage_Optimal_Smoothing 
N_Stage =
3
Residual_Smoothing_Epsilon = 
0.125
Residual_Smoothing_Gauss_Seidel_Iterations =
2
########################################
# 1 Yes, 0, No
